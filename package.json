{
  "name": "qbapi",
  "version": "0.0.2",
  "description": "qiibee API private ecosystem",
  "author": "Pascal Schindelholz <qiibee ag>",
  "license": "ISC",
  "private": true,
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/babel-node server.js",
    "startmon": "nodemon server.js",
    "lint": "eslint --cache .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  },
  "pre-commit": {
    "run": "precommit",
    "silent": true
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "lodash"
    ]
  },
  "engines": {
    "node": ">= 8.0"
  },
  "devDependencies": {
    "nodemon": "^1.14.11",
    "lint-staged": "^4.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.5.3",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "20.0.3",
    "remark-cli": "3.0.1",
    "remark-lint": "6.0.0",
    "remark-preset-lint-recommended": "2.0.0"
  },
  "dependencies": {
    "@warren-bank/ethereumjs-tx-unsign": "^2.0.0",
    "abi-decoder": "^1.0.9",
    "async": "^2.6.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-preset-latest": "^6.24.1",
    "bignumber.js": "^6.0.0",
    "bitcore-ecies": "^1.0.3",
    "bitcore-lib": "^0.15.0",
    "bitcore-mnemonic": "^1.5.0",
    "body-parser": "^1.18.2",
    "eth-hd-wallet": "^0.3.0",
    "ethereum-input-data-decoder": "0.0.3",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-wallet": "^0.6.0",
    "express": "^4.16.2",
    "express-prettify": "0.0.10",
    "left-pad": "^1.2.0",
    "minimist": "^1.2.0",
    "solc": "^0.4.20",
    "swagger-jsdoc": "^1.9.7",
