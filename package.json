{
  "name": "qbapi",
  "version": "0.0.2",
  "description": "qiibee API",
  "author": "qiibee Foundation",
  "license": "ISC",
  "private": true,
  "main": "server.js",
  "scripts": {
    "test": "node_modules/jest/bin/jest.js __tests__/*.test.js",
    "start": "./node_modules/.bin/babel-node server.js",
    "startmon": "nodemon server.js",
    "precommit": "lint-staged",
    "lint": "eslint . --fix",
    "build": "./node_modules/babel-cli/bin/babel.js app.js server.js src test -d dist --source-maps"
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix",
      "git add"
    ]
  },
  "pre-commit": {
    "run": "precommit",
    "silent": true
  },
  "babel": {
    "presets": [
      "latest"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "lodash"
    ]
  },
  "engines": {
    "node": ">= 8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2018": "^1.0.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "jest": "^23.1.0",
    "lint-staged": "^4.0.3",
    "nodemon": "^1.14.11",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.4",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "@warren-bank/ethereumjs-tx-unsign": "^2.0.0",
    "abi-decoder": "^1.0.9",
    "async": "^2.6.0",
    "bignumber.js": "^6.0.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "eslint-plugin-prettier": "^2.6.0",
    "eth-hd-wallet": "^0.3.0",
    "ethereum-input-data-decoder": "0.0.3",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-wallet": "^0.6.0",
    "express": "^4.16.3",
    "express-prettify": "0.0.10",
    "http-status-codes": "^1.3.0",
    "morgan": "^1.9.0",
    "solc": "^0.4.20",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^2.0.15",
    "url": "^0.11.0",
    "web3": "^1.0.0-beta.29",
    "web3-eth-personal": "^1.0.0-beta.30",
    "jest": "^23.1.0"
  }
}
